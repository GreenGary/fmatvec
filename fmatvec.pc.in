prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: fmatvec
Description: fmatvec is a library for fast matrix-vector computations.
Requires: 
Version: @fmatvec_VERSION@
Libs: -L${libdir} @LIBDIRS@ -lfmatvec @LIBS@
Cflags: @INCDIRS@ -I${includedir} @DEFINES@


